

typeset -U path cdpath fpath manpath



for profile in ${(z)NIX_PROFILES}; do
  fpath+=($profile/share/zsh/site-functions $profile/share/zsh/$ZSH_VERSION/functions $profile/share/zsh/vendor-completions)
done

HELPDIR="/nix/store/axyjlzb9rd4kmc452jaq4sjb5da7iljs-zsh-5.8.1/share/zsh/$ZSH_VERSION/help"







path+="$HOME/.config/zsh/plugins/fzf-tab"
fpath+="$HOME/.config/zsh/plugins/fzf-tab"


# Oh-My-Zsh/Prezto calls compinit during initialization,
# calling it twice causes slight start up slowdown
# as all $fpath entries will be traversed again.
autoload -U compinit && compinit

source /nix/store/wrxpkqjsarw95bc4dhgnlqah4zj0r84l-zsh-autosuggestions-0.7.0/share/zsh-autosuggestions/zsh-autosuggestions.zsh

source /nix/store/vvdnc4ygccnsl1zp2lsv45bngyjjdm5w-zsh-syntax-highlighting-0.7.1/share/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh





if [[ -f "$HOME/.config/zsh/plugins/fzf-tab/fzf-tab.plugin.zsh" ]]; then
  source "$HOME/.config/zsh/plugins/fzf-tab/fzf-tab.plugin.zsh"
fi


# History options should be set in .zshrc and after oh-my-zsh sourcing.
# See https://github.com/nix-community/home-manager/issues/177.
HISTSIZE="10000"
SAVEHIST="10000"

HISTFILE="$HOME/.config/zsh/.zsh_history"
mkdir -p "$(dirname "$HISTFILE")"

setopt HIST_FCNTL_LOCK
setopt HIST_IGNORE_DUPS
setopt HIST_IGNORE_SPACE
unsetopt HIST_EXPIRE_DUPS_FIRST
setopt SHARE_HISTORY
unsetopt EXTENDED_HISTORY
setopt autocd

if [[ $options[zle] = on ]]; then
  . /nix/store/hzp8s6n46g4sqi2iaa8cs4x5sv0vyb6x-fzf-0.29.0/share/fzf/completion.zsh
  . /nix/store/hzp8s6n46g4sqi2iaa8cs4x5sv0vyb6x-fzf-0.29.0/share/fzf/key-bindings.zsh
fi

eval "$(/nix/store/8i9y40pq8a4lqirkj8rbq9mncssc7rnx-zoxide-0.8.0/bin/zoxide init zsh )"

if [[ $TERM != "dumb" && (-z $INSIDE_EMACS || $INSIDE_EMACS == "vterm") ]]; then
  eval "$(/Users/darius/.nix-profile/bin/starship init zsh)"
fi

eval "$(/nix/store/15p8v3gbvmhlpqwrshqgzzr7hx66igrn-direnv-2.30.3/bin/direnv hook zsh)"

zstyle ':completion:*:descriptions' format '%d'
zstyle ':fzf-tab:*' group-colors ''


# Aliases
alias ..='cd ..'
alias cat='/nix/store/gg51z1phnbpqg8x8yylqsa66g8h00cfv-bat-0.20.0/bin/bat --paging=never'
alias du='/nix/store/v1hjrm7jmz65pmmg20s5bcxcj2h2y6br-ncdu-1.16/bin/ncdu'
alias la='/nix/store/gy0bqcs9mcan8af47wakdylhal67dpy4-exa-0.10.1/bin/exa -a'
alias ll='/nix/store/gy0bqcs9mcan8af47wakdylhal67dpy4-exa-0.10.1/bin/exa -al'
alias ls='/nix/store/gy0bqcs9mcan8af47wakdylhal67dpy4-exa-0.10.1/bin/exa'
alias top='/nix/store/4mzawc017lj81daasjh8gvqp6vibbj5i-htop-3.1.2/bin/htop'

